{
  "name": "advisor-finder",
  "version": "1.0.0",
  "description": "Data server for advisor finder",
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node ./src/index.ts",
    "build": "cd client && yarn && yarn run build",
