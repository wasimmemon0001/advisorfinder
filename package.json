    "apollo-server-express": "^2.23.0",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "graphql": "^15.5.0",
    "nodemon": "^2.0.7",
    "react-loader-spinner": "^4.0.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "concurrently": "^6.0.2"
